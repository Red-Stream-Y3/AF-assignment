{
  "name": "AgroHelp",
  "description": "Deploy agrohelp to heroku",
  "keywords": ["agrohelp"],
  "website": "https://github.com/Red-Stream-Y3/AF-assignment",
  "repository": "https://github.com/Red-Stream-Y3/AF-assignment",
  "env": {
    "PORT": {
      "description": "Port to run the server",
      "value": "9120"
    },
    "MONGO_URI": {
      "description": "MongoDB URI to connect the database",
      "value": "mongodb+srv://"
    },
    "JWT_SECRET": {
      "description": "JWT SECRET to sign the tokens",
      "value": "redstream"
    },
    "NODE_ENV": {
      "description": "Mode to run the server in production or development mode",
      "value": "production"
    }
  },
  "buildpacks": {
    "url": "heroku/nodejs"
  }
}
